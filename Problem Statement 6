#include<iostream.h>
using namespace std;
class Raja
{
    public:
    void task(int x)
    {
        cout<<"Tasks-wash the clothes,";
    }
    void task(int y,int z)
    {
        cout<<"read,";
    }
    void task(int p,int q,int r)
    {
        cout<<"book two tickets etc.";
    }
};
int main()
{
    Raja obj;
    obj.task(1);
    obj.task(1,1);
    obj.task(1,1,1);
}
